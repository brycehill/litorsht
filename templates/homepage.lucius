
/* Home Page Styles*/

.parkList {
  padding: 0;
  display: grid;
  grid-template-rows: repeat(5, 200px);
grid-template-columns:max-content;
  grid-column-gap: 10px;
  grid-row-gap: 15px;
  width: 100%;
  grid-auto-flow: column;
}

.parkItem {
  list-style-type:none;
  padding: 18px 20px;
  border-radius: 4px;
  box-shadow:2px 2px 8px lightgray;
}

